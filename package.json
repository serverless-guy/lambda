    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.6.0",
    "mocha": "^8.1.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
